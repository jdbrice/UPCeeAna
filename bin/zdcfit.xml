<?xml version="1.0" encoding="UTF-8"?>
<config>
	
	<mod>AuAu</mod>
	<title>@titles.{mod}</title>
	<Task name="ZDCFitter" type="ZDCFitter" config="" nodePath="ZDCFitter"  />
	<!-- <mod>AuAu</mod> -->


	<ZDCFitter>
		<input>
			<TFile url="{mod}.root" name="data" />
		</input>

		<output>
			<TFile url="fit_ZDC_{mod}.root" />
		</output>

		<histograms>
			
		</histograms>

		
	</ZDCFitter>


	<style>
		<hist axisdigits="4" line-width="2" line-color="#00F" min="0.001"/>
		<fitsum line-width="3" line-color="#000" npx="1000" />
		<fgn1 line-width="3" line-color="#F00" />
		<fgn2 line-width="3" line-color="#33F" />
		<fgn3 line-width="3" line-color="#0F0" />
		<fgn4 line-width="3" line-color="#F0F" />
	</style>

	<titles>
		<Run11AuAu>Run11 Au+Au</Run11AuAu>
		<Run10AuAu>Run10 Au+Au</Run10AuAu>
	</titles>


</config>
