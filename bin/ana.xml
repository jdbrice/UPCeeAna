<?xml version="1.0" encoding="UTF-8"?>
<config>
	<Task name="Analyzer" type="Analyzer" config="" nodePath="Analyzer"  />
	<mod>Run12UU</mod>
	<ver></ver>

	<p>
		<dTofCut>0.50</dTofCut>
		<XeeXpipi>4</XeeXpipi>

		<XeeCut>10</XeeCut>
		<XpipiCut>0</XpipiCut>

		<!-- <ZDCMode>1n1n</ZDCMode> -->

		<!-- <ZDCEast min="300" max="3000"/>
		<ZDCWest min="300" max="3000"/> -->

		<!-- Run12 UU 1n1n +/- 2 sigma -->
		<!-- <ZDCEast min="31" max="85" />
		<ZDCWest min="31" max="80"/> -->

		<!-- Run12 UU NOT 1n1n +/- 2 sigma -->
		<!-- <ZDCEast min="85" max="8500" />
		<ZDCWest min="85" max="8000"/> -->

		<!-- ALL -->
		<ZDCEast min="0" max="8500" />
		<ZDCWest min="0" max="8000"/>
		
	</p>
	
	<Analyzer>
		<EventLoop progress="true" />
		
		<input>
			<dst treeName="PairDst" url="/Users/jdb/bnl/work/upc/FemtoDst2PairDst/bin/slim_pair_dst_{mod}.root"  maxFiles="-1" />
		</input>

		<output>
			<TFile url="{mod}{ver}.root" />
		</output>

		<histograms>
			<Histo name="events" title="" bins_x="bins.events" />

			<Histo name="ZDCEast" title="ZDCEast" bins_x="bins.zdc" />
			<Histo name="ZDCWest" title="ZDCWest" bins_x="bins.zdc" />

			<Histo name="ZDCEastWest" title="ZDCEastWest" bins_x="bins.zdc" bins_y="bins.zdc" />
			<Histo name="ZDCEastWestSelected" title="ZDCEastWest" bins_x="bins.zdc" bins_y="bins.zdc" />
			<Histo name="ZDCEastWestAnalyzed" title="ZDCEastWest" bins_x="bins.zdc" bins_y="bins.zdc" />

			<Histo name="uls" title=";M_{ee} (GeV/c^{2}); dN/dM (GeV/c^{2})^{-1}" bins_x="bins.mass" bins_y="bins.pt" />
			<Histo name="ls" title=";M_{ee} (GeV/c^{2}); dN/dM (GeV/c^{2})^{-1}" bins_x="bins.mass" bins_y="bins.pt" />
			<Histo name="ulsid" title=";M_{ee} (GeV/c^{2}); dN/dM (GeV/c^{2})^{-1}" bins_x="bins.mass" bins_y="bins.pt" />
			<Histo name="lsid" title=";M_{ee} (GeV/c^{2}); dN/dM (GeV/c^{2})^{-1}" bins_x="bins.mass" bins_y="bins.pt" />
			
			<Histo name="lsid2" title=";M_{ee} (GeV/c^{2}); dN/dM (GeV/c^{2})^{-1}" bins_x="bins.mass" bins_y="bins.pt" />
			<Histo name="msquared" title=";p (GeV/c);M^{2}" bins_x="bins.p" bins_y="bins.mass" />

			<Histo name="aco" title=";M_{ee} (GeV/c^{2}); 1 - |#Delta #phi|/#pi" bins_x="bins.mass" bins_y="bins.aco" />
			<Histo name="asym" title=";M_{ee} (GeV/c^{2}); A" bins_x="bins.mass" bins_y="bins.asym" />

			<Histo name="ulsid2" title=";M_{ee} (GeV/c^{2}); dN/dM (GeV/c^{2})^{-1}" bins_x="bins.mass" bins_y="bins.pt" />
			<Histo name="rapid2" title=";M_{ee} (GeV/c^{2}); y^{ee}" bins_x="bins.mass" bins_y="bins.rap" />
			<Histo name="pt2id2" title=";M_{ee} (GeV/c^{2}); p_{T}^{2} (GeV/c)^{2}" bins_x="bins.mass" bins_y="bins.pt2" />
			
			<Histo name="mvsephi" title="; #phi;M_{ee} (GeV/c^{2})" bins_x="bins.phi" bins_y="bins.mass" />
			<Histo name="mvspphi" title="; #phi;M_{ee} (GeV/c^{2})" bins_x="bins.phi" bins_y="bins.mass" />
			<Histo name="mvsdphi" title="; #phi;M_{ee} (GeV/c^{2})" bins_x="bins.phi" bins_y="bins.mass" />

			<Histo name="mass1tof" title=";M_{ee} (GeV/c^{2}); dN/dM (GeV/c^{2})^{-1}" bins_x="bins.mass" />
			<Histo name="mass2tof" title=";M_{ee} (GeV/c^{2}); dN/dM (GeV/c^{2})^{-1}" bins_x="bins.mass" />

			<Histo name="pt1tof" title=";M_{ee} (GeV/c^{2}); dN/dM (GeV/c^{2})^{-1}" bins_x="bins.pt" />
			<Histo name="pt2tof" title=";M_{ee} (GeV/c^{2}); dN/dM (GeV/c^{2})^{-1}" bins_x="bins.pt" />

			<Histo name="ulsidtof" title=";M_{ee} (GeV/c^{2}); dN/dM (GeV/c^{2})^{-1}" bins_x="bins.mass" bins_y="bins.pt" />

			<Histo name="nSigE1_nSigE2" title=";n#sigma_{e1}; n#sigma_{e2}" bins_x="bins.nsig" bins_y="bins.nsig" />
			<Histo name="nSigE1_nSigE2_sel" title=";n#sigma_{e1}; n#sigma_{e2}" bins_x="bins.nsig" bins_y="bins.nsig" />
			<Histo name="nSigE1_nSigE2_cut" title=";n#sigma_{e1}; n#sigma_{e2}" bins_x="bins.nsig" bins_y="bins.nsig" />

			<Histo name="mNHitsFit" title="mNHitsFit" bins_x="bins.nhit" />
			<Histo name="mNHitsPoss" title="mNHitsFit" bins_x="bins.nhit" />
			<Histo name="mNHitsDeDx" title="mNHitsFit" bins_x="bins.nhit" />
			<Histo name="mDCA" title="DCA" bins_x="bins.dca" />
			<Histo name="mDCA_mPt" title="; p_{T}; DCA (cm)" bins_x="bins.pt" bins_y="bins.dca" />

			<Histo name="mNHits_mPhi_etap_pos" title="pos;#phi;nHitsFit" bins_x="bins.mPhi" bins_y="bins.nHits" />
			<Histo name="mNHits_mPhi_etap_neg" title="pos;#phi;nHitsFit" bins_x="bins.mPhi" bins_y="bins.nHits" />

			<Histo name="mNHits_mPhi_etan_pos" title="pos;#phi;nHitsFit" bins_x="bins.mPhi" bins_y="bins.nHits" />
			<Histo name="mNHits_mPhi_etan_neg" title="pos;#phi;nHitsFit" bins_x="bins.mPhi" bins_y="bins.nHits" />



			<!-- with TPC efficiency correction -->
			<Histo name="wulsid2" title=";M_{ee} (GeV/c^{2}); dN/dM (GeV/c^{2})^{-1}" bins_x="bins.mass" bins_y="bins.pt" />
			<Histo name="wvpulsid2" title=";M_{ee} (GeV/c^{2}); dN/dM (GeV/c^{2})^{-1}" bins_x="bins.mass" bins_y="bins.pt" />
			<Histo name="wrapid2" title=";M_{ee} (GeV/c^{2}); y^{ee}" bins_x="bins.mass" bins_y="bins.rap" />
			<Histo name="wpt2id2" title=";M_{ee} (GeV/c^{2}); p_{T}^{2} (GeV/c)^{2}" bins_x="bins.mass" bins_y="bins.pt2" />

			<Histo name="w1ulsid2" title=";M_{ee} (GeV/c^{2}); dN/dM (GeV/c^{2})^{-1}" bins_x="bins.mass" bins_y="bins.pt" />
			<Histo name="wvp1ulsid2" title=";M_{ee} (GeV/c^{2}); dN/dM (GeV/c^{2})^{-1}" bins_x="bins.mass" bins_y="bins.pt" />

			<!-- 3D efficiency weighting -->
			<Histo name="w3ulsid2" title=";M_{ee} (GeV/c^{2}); dN/dM (GeV/c^{2})^{-1}" bins_x="bins.mass" bins_y="bins.pt" />
			<Histo name="w3rapid2" title=";M_{ee} (GeV/c^{2}); y^{ee}" bins_x="bins.mass" bins_y="bins.rap" />
			<Histo name="w3pt2id2" title=";M_{ee} (GeV/c^{2}); p_{T}^{2} (GeV/c)^{2}" bins_x="bins.mass" bins_y="bins.pt2" />
			<Histo name="w3costhetaid2" title=";M_{ee} (GeV/c^{2}); costheta" bins_x="bins.mass" bins_y="bins.costheta" />
			<Histo name="w2costhetaid2" title=";M_{ee} (GeV/c^{2}); costheta" bins_x="bins.mass" bins_y="bins.costheta" />

			<Histo name="meanPt2" title=";M_{ee} (GeV/c^{2}); #sqrt{p_{T}^{2} } (MeV/c)" bins_x="bins.massSlice" />
			<Histo name="meanPt2Model" title=";M_{ee} (GeV/c^{2}); #sqrt{p_{T}^{2} } (MeV/c)" bins_x="bins.massSlice" />

			<Histo name="dedx" title=";p (GeV/c); dEdx" bins_x="bins.p" bins_y="bins.dedx" />
			<Histo name="dedxcut" title=";p (GeV/c); dEdx" bins_x="bins.p" bins_y="bins.dedx" />
			<Histo name="chipid" title=";#chi_{#pi#pi}; #chi_{ee}" bins_x="bins.chi" bins_y="bins.chi" />
			<Histo name="chipidSel" title=";#chi_{#pi#pi}; #chi_{ee}" bins_x="bins.chi" bins_y="bins.chi" />
			<Histo name="chipidcut" title=";#chi_{#pi#pi}; #chi_{ee}" bins_x="bins.chi" bins_y="bins.chi" />
			<Histo name="chipidcutSel" title=";#chi_{#pi#pi}; #chi_{ee}" bins_x="bins.chi" bins_y="bins.chi" />

			<Histo name="deltaTof" title="dtof" bins_x="bins.deltaTof" />
			<Histo name="deltaTofe" title="dtof" bins_x="bins.deltaTofe" />
			<Histo name="deltaLen" title="dtof" bins_x="bins.deltaLen" />
			<Histo name="ddeltaTof" title="dtof" bins_x="bins.deltaLen" />
			<Histo name="deltaPhi_pd1" title="" bins_x="bins.deltaPhi" />
			<Histo name="deltaPhi_pd2" title="" bins_x="bins.deltaPhi" />
			<Histo name="deltaPhi_pd1_fold" title="" bins_x="bins.deltaPhiFold" />
			<Histo name="deltaPhi_pd1_fold_mass" title="" bins_x="bins.mass" bins_y="bins.deltaPhiFold" />

			<Histo name="wdeltaPhi_pd1" title="" bins_x="bins.deltaPhi" />
			<Histo name="wdeltaPhi_pd2" title="" bins_x="bins.deltaPhi" />
			<Histo name="wdeltaPhi_pd1_fold" title="" bins_x="bins.deltaPhiFold" />
			<Histo name="wdeltaPhi_pd1_2fold" title="" bins_x="bins.deltaPhi2Fold" />

			<Histo name="wdeltaPhi_pd1_fold_mass" title="" bins_x="bins.mass" bins_y="bins.deltaPhiFold" />
			<Histo name="wdeltaPhi_pd1_fold_pt" title="" bins_x="bins.pt" bins_y="bins.deltaPhiFold" />

			<!-- Full mass range -->
			<Histo name="wdeltaPhi_pd1_fold_all" title="" bins_x="bins.deltaPhiFold" />
			<Histo name="wdeltaPhi_pd1_2fold_all" title="" bins_x="bins.deltaPhi2Fold" />

			<Histo name="deltaAngle_pd1" title="" bins_x="bins.deltaPhi" />
			<Histo name="deltaAngle_pd2" title="" bins_x="bins.deltaPhi" />
			
			<!-- <Histo name="ddeltaTofvsmaxp" title="dtof" bins_x="bins.pp" bins_y="bins.deltaTof" />
			<Histo name="ddeltaTofvsminp" title="dtof" bins_x="bins.pp" bins_y="bins.deltaTof" />
			<Histo name="ddeltaTofvssump" title="dtof" bins_x="bins.pp" bins_y="bins.deltaTof" /> -->
			<!-- <Histo name="ddeltaTofevsp" title="dtof" bins_x="bins.pp" bins_y="bins.deltaTof" /> -->

			<Histo name="xeedTof" title="xeedTof" bins_x="bins.chi" bins_y="bins.deltaTof" />
			<Histo name="xeedTofSel" title="xeedTof" bins_x="bins.chi" bins_y="bins.deltaTof" />
			<Histo name="xpipidTof" title="xpipidTof" bins_x="bins.chi" bins_y="bins.deltaTof" />
		</histograms>
	</Analyzer>


	<bins>
		<events min="0" max="10" width="1"/>
		<mass min="0" max="4" width="0.001" />
		<pt min="0" max="2.0" width="0.001" />
		<pt2 min="0" max="0.5" width="0.0001" />
		<aco min="0" max="1.0" width="0.001" />
		<asym min="0" max="1.0" width="0.001" />
		<zdc min="0" max="1200" width="5" />
		<!-- <pt2>
			0.0000, 0.0010, 0.0020, 0.0030, 0.0040, 0.0050, 0.0060, 0.0070, 0.0080, 0.0090, 
			0.0100, 0.0200, 0.0300, 0.0400, 0.0500, 0.0600, 0.0700, 0.0800, 0.0900,
			0.1000, 0.2000, 0.3000, 0.4000, 0.5000
		</pt2> -->

		<massSlice> 0.4, 0.76, 1.2, 2.6 </massSlice>
		<rap min="-2.0" max="2.0" width="0.1" />
		<p min="-3.0" max="3.0" width="0.005" />
		<pp min="0.0" max="2.0" width="0.01" />
		<dedx min="-15.0" max="15.0" width="0.005" />
		<chi min="0" max="200.0" width="0.1" />
		<nsig min="-10" max="10" width="0.1" />
		<phi min="-3.2" max="3.2" width="0.05" />
		<mPhi min="-3.14" max="3.14" width="0.314" />
		<nHits min="0" max="50" width="1" />
		<costheta min="0" max="1.0" width="0.01" />

		<deltaTof min="-15" max="15" width="0.01" />
		<deltaTofe min="-50" max="50" width="0.01" />
		<deltaLen min="-50" max="50" width="0.01" />
		<deltaPhi min="-3.141" max="3.141" nBins="50" />
		<deltaPhiFold min="0" max="1.57" nBins="15" />
		<deltaPhi2Fold min="0" max="0.785" nBins="15" />

		<nhit min="0" max="50" width="1" />
		<dca min="0" max="5" width="0.05" />
	</bins>


</config>
